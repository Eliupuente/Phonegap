<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.jquery" version="1.3.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
<preference name="phonegap-version" value="cli-9.0.0" />
  <name>Aplicacion 8</name>
  <description>
      Esta es una App de PhoneGap Hola Mundo ft jquery
  </description>
  <author email="Juan Eliu Gonzalez Puente" href="http://cordova.io">
      Juan Eliu Gonzalez Puente
  </author>
  <content src="index.html" />
  <plugin name="cordova-plugin-whitelist" spec="1.3.4" />
  <plugin name="cordova-plugin-device-motion" spce="1"/>
  <plugin name="cordova-plugin-dialogs"/> 
  <plugin name="cordova-plugin-device"/>
  <plugin name="cordova-plugin-camera"/>
  <plugin name="cordova-plugin-audioinput"/>
  <plugin name="cordova-plugin-mediacapture"/>
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <platform name="android">
      <allow-intent href="market:*" />
  </platform>
  <platform name="ios">
      <allow-intent href="itms:*" />
      <allow-intent href="itms-apps:*" />
  </platform>
</widget>
